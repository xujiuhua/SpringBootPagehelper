<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.johuer.dao.UserDao">

    <select id="getAll" resultType="com.johuer.domain.User">
        SELECT
          id, name, age, password
        FROM USER
    </select>

    <select id="getByPage" resultType="com.johuer.domain.User">
        SELECT
          id, name, age, password
        FROM USER
    </select>

    <select id="getByPageObj" resultType="com.johuer.domain.User">
        SELECT
          id, name, age, password
        FROM USER
    </select>

</mapper>